# the problem caused by kubeflow installation, using: 
kubectl delete apiservice v1beta1.webhook.cert-manager.io

NAMESPACE=auth
#open proxy assume the wanted namespace is auth
nohup kubectl proxy &

kubectl get namespace ${NAMESPACE} -o json > tmp.json

#get rid of finalizer "kubenetes"

curl -k -H "Content-Type: application/json" -X PUT --data-binary @tmp.json \
http://127.0.0.1:8001/api/v1/namespaces/${NAMESPACE}/finalize

